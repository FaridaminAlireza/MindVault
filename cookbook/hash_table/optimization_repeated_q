arr = [2,4,6,8,10]
prefix = {}
running = 0
for i, val in enumerate(arr):
    running += val
    prefix[i] = running

queries = [ (0,2), (1,3), (0,4) ]
for L,R in queries:
    print(prefix[R] - prefix.get(L-1,0))

# Works even if indices are not consecutive

# If L = 0, then prefix[L-1] would be prefix[-1] (wrong).
# So we handle it separately:
